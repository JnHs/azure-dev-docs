        "chef/**/*.md": "tarcher",
        "chef/**/*.yml": "tarcher",
        "github/**/*.md": "ushan",
        "github/**/*.yml": "ushan",
        "go/**/*.md": "rotabor",
        "go/**/*.yml": "rotabor",
        "java/**/*.md": "karler",
        "java/**/*.yml": "karler",
        "javascript/**/*.md": "diberry",
        "javascript/**/*.yml": "diberry",
        "jenkins/**/*.md": "tarcher",
        "jenkins/**/*.yml": "tarcher",
        "mobile-apps/**/*.md": "miparker",
        "mobile-apps/**/*.yml": "miparker",
        "python/**/*.md": "rotabor",
        "python/**/*.yml": "rotabor",
        "terraform/**/*.md": "tarcher",
        "terraform/**/*.yml": "tarcher",
        "intro/**/*.md": "mcleans",
        "intro/**/*.yml": "mcleans",
        "ai/**/*.md": "johalexander",
        "ai/**/*.yml": "johalexander",
        "github-copilot-azure/**/*.md": "rotabor",
        "github-copilot-azure/**/*.yml": "rotabor"
