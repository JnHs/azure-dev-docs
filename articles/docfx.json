        "github-copilot-azure/**/*.md": "bobtabor-msft",
        "github-copilot-azure/**/*.yml": "bobtabor-msft"
      },
      "manager": {
        "**/*.md": "mcleans",
        "**/*.yml": "mcleans"
      },
      "ms.author": {
        "**/*.md": "mcleans",
        "**/*.yml": "mcleans",
        "ansible/**/*.md": "tarcher",
        "ansible/**/*.yml": "tarcher",
        "azure-developer-cli/**/*.md": "alexwolf",
